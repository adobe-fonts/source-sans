	lookup MAJUCSULE_ACCENTS_CMB_CONTEXT { # substitutes combining marks for miniscules with forms for majuscules
		sub [ @LETTERS_MAJUSCULE_ALL @LETTERS_MINISCULE_LATIN_ASCENDERS ] @MARKS_CMB_LC' by @MARKS_CMB_CASE;
		sub @MARKS_CMB_CASE @MARKS_CMB_LC' by @MARKS_CMB_CASE;
		sub [ @LETTERS_MINISCULE_LATIN_ASCENDERS t esh ] breveinverteddoublecmb' by breveinverteddoublecmb.a;
		sub breveinverteddoublecmb' [ @LETTERS_MINISCULE_LATIN_ASCENDERS t esh ] by breveinverteddoublecmb.a;
	} MAJUCSULE_ACCENTS_CMB_CONTEXT;

} case;

