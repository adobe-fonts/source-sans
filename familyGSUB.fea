feature locl { # Localized Forms

	script latn;
		language dflt;
			lookup GREEK_TO_IPA;

		language ATH include_dflt;
			lookup OGONEK_ALTERNATES;

		language NAV include_dflt;
			lookup OGONEK_ALTERNATES;

		language NSM include_dflt;
			lookup ENG_UC_ALTERNATE;

		language SKS include_dflt;
			lookup ENG_UC_ALTERNATE;

		include (../../locl-IPPH-APPH.fea); # imports IPA lookups (which differ between upright and italic)

	script grek;
		sub [ acutecmb gravecmb commaturnedabovecmb commaabovecmb dieresisgrave ] by [ tonoscmb variacmb aspercmb leniscmb dialytikagravecmb ]; # combining marks

	script cyrl;

		language dflt;
			lookup CYRILLIC_BREVE { # heavy terminals (Cyrillic preference)
				sub [ brevecmb brevecmb.cap ] by [ brevecmb.cyr brevecmb.cyrcap ]; # combining marks
			} CYRILLIC_BREVE;

		language BGR include_dflt; # Bulgarian
			lookup CYRILLIC_TRIANGULAR_ALTERNATES;
			lookup CYRILLIC_LC_ASCENDING_ALTERNATES;
			include (../../locl_BGR.fea); # imports Bulgarian specific lookups (which differ between upright and italic)

		language MKD include_dflt; # Macedonian
			include (../../locl_MKD.fea); # imports Macedonian specific lookups (which differ between upright and italic)

		language SRB include_dflt; # Serbian
			include (../../locl_SRB.fea); # imports Serbians specific lookups (which differ between upright and italic)

} locl;

