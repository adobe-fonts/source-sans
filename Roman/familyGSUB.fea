@IPA_GRK_LC = [ alpha beta gamma iota upsilon phi chi ]; # Minion 3 includes theta
@IPA_LAT_LC = [ alphalatin betalatin gammalatin iotalatin upsilonlatin philatin chilatin ];

feature locl { # Localized Forms

	script latn;
		language dflt;
			lookup GREEK_TO_IPA { # substitutes Greek characters by Latin characters
				sub @IPA_GRK_LC by @IPA_LAT_LC;
			} GREEK_TO_IPA;

		language ATH include_dflt;
			lookup OGONEKS_ATH {
				sub [Aogonek Eogonek aogonek eogonek uogonek] by [Aogonek.d Eogonek.d aogonek.d eogonek.d uogonek.d];
			} OGONEKS_ATH;

		language NAV include_dflt;
			lookup OGONEKS_ATH;

		language TRK include_dflt;
			lookup I_TURKISH {
				sub i by i.trk;
			} I_TURKISH;

		language AZE include_dflt;
			lookup I_TURKISH;

		language CRT include_dflt;
			lookup I_TURKISH;

		language NSM include_dflt;
			lookup ENG_SAMI { # preferred Eng shape for Sami languages
				sub Eng by Eng.a;
			} ENG_SAMI;

		language SKS include_dflt;
			lookup ENG_SAMI;

#		language IPPH include_dflt;
#			lookup A_IPA { # ITALIC ONLY substitutes single-storey a with double-storey variants
#				   sub [ a a.sups ] by [ a.a a.supa ]; # Minion 3 includes ae
#		} A_IPA;
#
#		language APPH include_dflt;
#			lookup A_IPA;

	script grek;
		sub [acutecmb gravecmb commaturnedabovecmb commaabovecmb] by [tonoscmb variacmb aspercmb leniscmb]; # combining marks

	script cyrl;
		sub [brevecmb brevecmb.cap] by [brevecmb.cyr brevecmb.cyrcap]; # combining marks

		language SRB include_dflt;
			lookup BE_LC_SRB {
				sub be by be.srb;
			} BE_LC_SRB;

} locl;

